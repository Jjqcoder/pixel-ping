generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model users {
  ID       Int     @id @default(autoincrement())
  USERNAME String? @db.VarChar(255)
  PASSWORD String? @db.VarChar(255)
}
